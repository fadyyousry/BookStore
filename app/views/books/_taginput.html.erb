<div class="form-group">
  <%= label_tag plural, t("default.#{singular}.name.other"), class: "col-md-3 control-label" %>
  <div class="col-md-6">
    <% val = nil %>
    <% list.each do |value| %>
        <% val = (val || "") + value[:name] + ',' %> 
    <% end %>
    <%= text_field_tag "#{plural}[names]", val,
     {id: "#{plural}-input", 'data-role' => "tagsinput",
      'data-tag-class' => "label label-primary", class: "form-control"} %>
  </div>
</div>